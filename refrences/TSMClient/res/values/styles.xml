<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="CMBAnimBottom" parent="@android:style/Animation">
        <item name="android:windowEnterAnimation">@anim/cmbkb_push_bottom_in</item>
        <item name="android:windowExitAnimation">@anim/cmbkb_push_bottom_out</item>
    </style>
    <style name="CmbDialogStyleBottomDark" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowAnimationStyle">@style/CMBAnimBottom</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>
    <style name="CmbDialogStyleBottom" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowAnimationStyle">@style/CMBAnimBottom</item>
        <item name="android:backgroundDimEnabled">false</item>
    </style>
    <style name="CmbDialogStyle">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>
    <style name="ActionBar" parent="@miui:style/Widget.ActionBar" />
    <style name="ActionBar.Main" parent="@style/ActionBar">
        <item name="android:displayOptions">showCustom</item>
    </style>
    <style name="ActionBar.NoHomeAsUp" parent="@style/ActionBar">
        <item name="android:displayOptions">showTitle</item>
    </style>
    <style name="ActionBar.HomeAsUp" parent="@style/ActionBar">
        <item name="android:displayOptions">homeAsUp|showCustom|showTitle</item>
    </style>
    <style name="ActionBar.HomeAsUp.Transparent" parent="@style/ActionBar.HomeAsUp">
        <item name="android:background">@miui:color/transparent</item>
        <item name="android:titleTextStyle">@style/TextAppearance.Title</item>
    </style>
    <style name="ActionBar.Main.TabView" parent="@style/ActionBar.Main">
        <item name="android:gravity">center</item>
        <item name="android:background">@null</item>
    </style>
    <style name="TextAppearance" />
    <style name="TextAppearance.Title" parent="@style/TextAppearance">
        <item name="android:textSize">@miui:dimen/normal_text_size</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@android:color/white</item>
    </style>
    <style name="TextAppearance.Title.Black" parent="@style/TextAppearance.Title">
        <item name="android:textSize">@miui:dimen/normal_text_size</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">?miui:actionBarTitleColor</item>
    </style>
    <style name="TextAppearance.Content" parent="@style/TextAppearance">
        <item name="android:textSize">@dimen/tsm_text_size_content_normal</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="TextAppearance.Content.Dark" parent="@style/TextAppearance.Content">
        <item name="android:textColor">@color/dark</item>
    </style>
    <style name="TextAppearance.Content.Light" parent="@style/TextAppearance.Content">
        <item name="android:textColor">@color/gray</item>
    </style>
    <style name="TextAppearance.Content.Red" parent="@style/TextAppearance.Content">
        <item name="android:textColor">@color/red</item>
    </style>
    <style name="TextAppearance.Content.Small" parent="@style/TextAppearance.Content">
        <item name="android:textSize">@dimen/tsm_text_size_content_small</item>
    </style>
    <style name="TextAppearance.Large" parent="@style/TextAppearance">
        <item name="android:textSize">@dimen/card_recharge_finish_text_size_large</item>
    </style>
    <style name="TextAppearance.Normal" parent="@style/TextAppearance">
        <item name="android:textSize">@dimen/card_recharge_finish_text_size_normal</item>
    </style>
    <style name="TextAppearance.Normal.Grey" parent="@style/TextAppearance.Normal">
        <item name="android:textColor">@color/gray</item>
    </style>
    <style name="TextAppearance.Normal.Green" parent="@style/TextAppearance.Normal">
        <item name="android:textColor">@color/green</item>
    </style>
    <style name="TextAppearance.Large.Title" parent="@style/TextAppearance.Large">
        <item name="android:textSize">@dimen/card_recharge_nfc_card_name_size</item>
        <item name="android:textStyle">italic</item>
        <item name="android:textColor">@color/white</item>
    </style>
    <style name="TextAppearance.TabText" parent="@miui:style/Widget.ActionBar.TabText">
        <item name="android:textColor">@color/tab_text_light</item>
    </style>
    <style name="Button" />
    <style name="Button.FullWidth" parent="@style/Button">
        <item name="android:gravity">center</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="Widget.Button.Warning" parent="@miui:style/Widget.Button.Warning">
        <item name="android:textSize">@dimen/tsm_text_size_button_normal</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="Widget.ListView.Item.SingleLine" parent="@miui:style/Widget.ListView.Item.SingleLine" />
    <style name="Widget.ListView.Item.DoubleLine" parent="@miui:style/Widget.ListView.Item.DoubleLine" />
    <style name="TextAppearance.List.Primary" parent="@miui:style/TextAppearance.List.Primary" />
    <style name="TextAppearance.List.Secondary" parent="@miui:style/TextAppearance.List.Secondary" />
    <style name="TextAppearance.List.Category" parent="@miui:style/TextAppearance.PreferenceCategory" />
    <style name="TextAppearance.Digit" parent="@style/TextAppearance" />
    <style name="TextAppearance.Digit.EditText" parent="@style/TextAppearance.Digit" />
    <style name="TextAppearance.FormItem" parent="@style/TextAppearance">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="TextAppearance.FormItem.Title" parent="@style/TextAppearance.FormItem">
        <item name="android:textSize">@dimen/bank_card_trans_element_item_text_size</item>
        <item name="android:textColor">@color/bank_card_trans_element_item_title</item>
        <item name="android:layout_width">@dimen/bank_card_item_text_width</item>
    </style>
    <style name="TextAppearance.FormItem.Content" parent="@style/TextAppearance.FormItem">
        <item name="android:textSize">@dimen/bank_card_trans_element_item_text_size</item>
        <item name="android:textColor">@color/dark</item>
    </style>
    <style name="TextAppearance.BankInfo" parent="@style/TextAppearance">
        <item name="android:textSize">@dimen/tsm_text_size_content_normal</item>
        <item name="android:textColor">@color/bank_card_info</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="TextAppearance.Keyboard.Title" parent="@miui:style/TextAppearance.Medium">
        <item name="android:textColor">@color/safe_keyboard_title_color</item>
    </style>
    <style name="TextAppearance.Hint" parent="@style/TextAppearance">
        <item name="android:textSize">@dimen/tsm_text_size_content_tiny</item>
        <item name="android:textColor">@color/gray</item>
    </style>
    <style name="Widget.EditText.Clearable" parent="@miui:style/Widget.EditText.Search">
        <item name="android:background">@drawable/edit_text_bg</item>
        <item name="android:paddingLeft">@dimen/edit_text_padding</item>
        <item name="android:paddingRight">@dimen/edit_text_padding</item>
    </style>
    <style name="Widget.BankCardListItem" parent="@style/Widget.ListView.Item.DoubleLine">
        <item name="android:layout_height">@dimen/bank_card_list_item_height</item>
    </style>
    <style name="Widget.ProgressBar" parent="@miui:style/Widget.Dark.ProgressBar" />
    <style name="Keyboard" />
    <style name="Keyboard.Key" parent="@style/Keyboard">
        <item name="android:textSize">@dimen/tsm_text_size_huge</item>
        <item name="android:textColor">@color/safe_keyboard_key_text_color</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/safe_keyboard_key</item>
        <item name="android:focusable">true</item>
        <item name="android:clickable">true</item>
        <item name="android:layout_width">0.0dip</item>
        <item name="android:layout_height">@dimen/safe_keyboard_key_height</item>
        <item name="android:layout_weight">1.0</item>
    </style>
    <style name="Keyboard.Key.Special" parent="@style/Keyboard.Key">
        <item name="android:background">@drawable/safe_keyboard_key_special</item>
    </style>
    <style name="safe_keyboard_style">
        <item name="android:windowEnterAnimation">@anim/up_safe_keyboard_enter</item>
        <item name="android:windowExitAnimation">@anim/up_safe_keyboard_exit</item>
    </style>
    <style name="Theme.Light" parent="@miui:style/Theme.Light" />
    <style name="Theme.Light.Main" parent="@style/Theme.Light">
        <item name="android:actionBarStyle">@style/ActionBar.Main</item>
    </style>
    <style name="Theme.Light.NoHomeAsUp" parent="@style/Theme.Light">
        <item name="android:actionBarStyle">@style/ActionBar.NoHomeAsUp</item>
        <item name="android:displayOptions">showTitle</item>
    </style>
    <style name="Theme.Light.HomeAsUp" parent="@style/Theme.Light">
        <item name="android:actionBarStyle">@style/ActionBar.HomeAsUp</item>
    </style>
    <style name="Theme.Light.HomeAsUp.Transparent" parent="@style/Theme.Light.HomeAsUp">
        <item name="android:actionBarStyle">@style/ActionBar.HomeAsUp.Transparent</item>
        <item name="android:homeAsUpIndicator">@drawable/action_bar_back_light</item>
        <item name="miui:windowActionBarOverlay">true</item>
        <item name="miui:windowTranslucentStatus">transparentDark</item>
    </style>
    <style name="Theme.Light.NoTitle" parent="@miui:style/Theme.Light.NoTitle" />
    <style name="Theme.Light.NoTitle.Light" parent="@style/Theme.Light.NoTitle">
        <item name="miui:windowTranslucentStatus">transparentDark</item>
    </style>
    <style name="Theme.Light.Dialog.NoTitle" parent="@miui:style/Theme.Light.Dialog">
        <item name="miui:windowActionBar">false</item>
        <item name="miui:windowActionBarOverlay">false</item>
    </style>
    <style name="Theme.Light.Transparent" parent="@miui:style/Theme.Translucent.NoTitleBar">
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@null</item>
        <item name="miui:startingWindowOverlay">@null</item>
    </style>
    <style name="Theme.Light.NoHome" parent="@style/Theme.Light">
        <item name="android:actionBarStyle">@style/ActionBar.Main.TabView</item>
        <item name="android:displayOptions">none</item>
        <item name="android:actionBarTabTextStyle">@style/TextAppearance.TabText</item>
    </style>
</resources>
